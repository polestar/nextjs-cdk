{
  "name": "@plstr/nextjs-cdk",
  "version": "0.0.0-development",
  "author": "Anders Quist <anders.quist@polestar.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prepare": "is-ci || husky install",
    "commit": "git-cz",
    "prebuild": "yarn clean",
    "build": "yarn build:ts && yarn copy-sharp-modules && yarn rollup",
    "build:ts": "tsc -p tsconfig.build.json && tsc -p tsconfig.module.json",
    "clean": "yarn rimraf dist",
    "copy-sharp-modules": "ts-node --project tsconfig.scripts.json scripts/copy-sharp-modules.ts",
    "format": "prettier --ignore-path .gitignore --write src tests",
    "rollup": "rollup --config",
    "pretest": "yarn build",
    "semantic-release": "semantic-release",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "jest --verbose --coverage ./tests",
    "test:unit:default": "jest --verbose --coverage",
    "test:e2e": "ts-node --project tsconfig.scripts.json ./tests/e2e/run-tests.ts"
  },
  "devDependencies": {
